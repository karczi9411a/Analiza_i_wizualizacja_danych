Funkcja <- function()
{
Aktualizuj_wykres <- function(h,...) {
if(svalue(lista3) == "Punkty_oddalone(x)")
{
plot(main = "Punkty_oddalone(x)",(baza[,(which(colnames(baza) == svalue(lista1)))]),ylab=svalue(lista1),xlab="Pacjenci")
}
else if(svalue(lista3) == "Histogram(x)")
{
hist(main = "Histogram(x)",(baza[,(which(colnames(baza) == svalue(lista1)))]),ylab="Ilosc",xlab = svalue(lista1))
}
else if(svalue(lista3) == "Pudełkowy(x,y)")
{
boxplot(main = "Pudełkowy(x,y)",(baza[,(which(colnames(baza) == svalue(lista1)))]),(baza[,(which(colnames(baza) == svalue(lista2)))]),ylab="Wartosci",xlab="Wybrane_kolumny")
}
else if(svalue(lista3) == "Rozrzutu(x,y)")
{
plot(main = "Rozrzutu(x,y)",(baza[,(which(colnames(baza) == svalue(lista1)))]),(baza[,(which(colnames(baza) == svalue(lista2)))]),ylab=svalue(lista1),xlab=svalue(lista2))
}
else if(svalue(lista3) == "Regresja_liniowa(x,y)")
{
plot(main = "Regresja_liniowa(x,y)",(baza[,(which(colnames(baza) == svalue(lista1)))]),(baza[,(which(colnames(baza) == svalue(lista2)))]),ylab=svalue(lista1),xlab=svalue(lista2))
abline(lm((baza[,(which(colnames(baza) == svalue(lista2)))]) ~ (baza[,(which(colnames(baza) == svalue(lista1)))])))
}
}
#plot(baza$Waga,baza$plytki.krwi)
#abline(lm(baza$Waga ~ baza$plytki.krwi))
window2 <- gwindow("Aplikaca - AWD(Wykresy)")
wgroup <- ggroup(horizontal=FALSE, cont=window2)
lista1 <- gcombobox(c(names(baza[,4:79])),container = wgroup,handler = Aktualizuj_wykres)
lista2 <- gcombobox(c(names(baza[,4:79])),container = wgroup,handler = Aktualizuj_wykres)
lista3 <- gcombobox(c("Punkty_oddalone(x)","Histogram(x)","Pudełkowy(x,y)","Rozrzutu(x,y)","Regresja_liniowa(x,y)"),container = wgroup ,handler = Aktualizuj_wykres)
add(wgroup, ggraphics())
Aktualizuj_wykres()
}
Funkcja()
